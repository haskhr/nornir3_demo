from typing import Dict, Optional


class AcmeOSAPI:
    def __init__(
        self,
        hostname: Optional[str],
        username: Optional[str],
        password: Optional[str],
        port: Optional[int],
    ) -> None:
        self.hostname = hostname
        self.username = username
        self.password = password
        self.port = port

    def open(self) -> None:
        pass

    def close(self) -> None:
        pass

    def get_version(self) -> Dict[str, str]:
        return {
            "os_version": "5.2",
            "revision": "3",
            "full_version": "5.2.3",
        }
